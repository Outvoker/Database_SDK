{
    "compilerOptions": {
        // "module": "esnext",
        "lib": ["es5", "es2015", "es2016", "dom"],
        "target": "es5",
        "downlevelIteration": true,
        "noImplicitAny": false,
        "removeComments": false,
        "preserveConstEnums": true,
        "sourceMap": true,
        "outDir": "build",
        "declaration": true,
        "declarationDir": "build"
    },
    "target": "index.ts",
    "exclude": [
        "node_modules",
        "test.*"
    ]
}